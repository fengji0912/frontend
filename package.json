    "@fortawesome/react-fontawesome": "^0.2.0",
    "@next/bundle-analyzer": "^14.1.0",
    "@nextui-org/react": "^2.4.2",
    "@socialgouv/matomo-next": "^1.8.1",
    "citation-js": "^0.7.8",
    "csl-json": "^0.1.0",
    "framer-motion": "^10.18.0",
    "js-md5": "^0.8.3",
    "jspdf": "^2.5.2",
    "ky": "^1.2.2",
    "lodash": "^4.17.21",
    "next": "^14.2.5",
    "next-client-cookies": "^1.1.1",
    "next-intl": "^3.14.1",
    "next-runtime-env": "^3.2.1",
    "next-themes": "^0.3.0",
    "nextjs-toploader": "^1.6.6",
    "nuqs": "^1.17.4",
    "openai": "^4.55.7",
    "pluralize": "^8.0.0",
    "pocketbase": "^0.20.3",
    "qs": "^6.12.0",
    "react": "^18.3.1",
    "react-content-loader": "^6.2.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "react-scroll-sync": "^0.11.2",
    "react-select": "^5.8.0",
    "react-select-async-paginate": "^0.7.3",
    "react-share": "^5.1.0",
    "react-slick": "^0.30.2",
    "react-string-replace": "^1.1.1",
    "react-use": "^17.5.0",
    "sharp": "^0.33.3",
    "slick-carousel": "^1.8.1",
